{
	"schemaVersion": 1,
	"id": "webcamcontrols",
	"version": "${version}",
	"name": "Webcam Controls",
	"description": "Acts as the middleman between Python and Java for my Webcam controlled Minecraft",
	"authors": [
		"vindognz"
	],
	"contact": {
		"homepage": "https://github.com/vindognz",
		"sources": "https://github.com/vindognz/webcam-controlled-minecraft"
	},
	"license": "CC0-1.0",
	"icon": "assets/webcamcontrols/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"me.sharp.webcamcontrols.WebcamControls"
		],
		"fabric-datagen": [
			"me.sharp.webcamcontrols.WebcamControlsDataGenerator"
		],
		"client": [
			"me.sharp.webcamcontrols.WebcamControlsClient"
		]
	},
	"mixins": [
		"webcamcontrols.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}